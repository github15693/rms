<div class="page-header">
  <%= link_to bulletins_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_bulletin_path(@bulletin), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show bulletin</h1>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @bulletin.title %></dd>

  <dt>Date:</dt>
  <dd><%= @bulletin.date %></dd>

  <dt>Content:</dt>
  <dd><%= @bulletin.content %></dd>

  <dt>Send notify:</dt>
  <dd><%= @bulletin.send_notify %></dd>

  <dt>Condo:</dt>
  <dd><%= @bulletin.condo_id %></dd>

</dl>
