<div class="modal-header">
  <h4 class="modal-title"><%= t('feedback.index.delete_feedback')%></h4>
</div>
<div class="modal-body">
  <p><%= t('feedback.index.confirm_delete') %></p>
</div>
<div class="modal-footer">
  <%= link_to t('feedback.index.btn_confirm'), feedback_path(@feedback), { class: 'btn btn-danger', method: :delete, remote: true } %>
  <%= link_to  t('feedback.index.btn_cancel'), '#', { class: 'btn btn-default', data: { dismiss: 'modal'} } %>
</div>
